project 'ilegra-marvel.xcodeproj'

platform :ios, '11'
inhibit_all_warnings!
use_frameworks!

def shared_pods
    # Network
    pod 'Alamofire', '~> 4.5'
    pod 'Kingfisher', '~> 4.0'

    #Mapper
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper'
    
    #Crypt 
    pod 'CryptoSwift'
end

target 'ilegra-marvel' do
    
    use_frameworks!
    # Shared pods
    shared_pods
end
