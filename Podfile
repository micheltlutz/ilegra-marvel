project 'ilegra-marvel.xcodeproj'

platform :ios, '11'
inhibit_all_warnings!
use_frameworks!

def shared_pods
    # Network
    pod 'Alamofire', '~> 4.5'
    pod 'SDWebImage'

    #Mapper
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper'
end



target 'ilegra-marvel' do
    # Shared pods
    shared_pods
end
